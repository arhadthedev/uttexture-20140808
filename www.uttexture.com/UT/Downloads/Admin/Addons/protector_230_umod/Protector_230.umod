␀␀␀␀␀␀␀␀␀␀y[class Protector  expands Mutator config(protector230);
//
// Protector 2.30 by Major Disaster
//
// Protect players from respawn fragging for some seconds
//
// New in this version: 
//
// Mod Menu
// Redundant translucency code (workaround for UT bug)
// Translucency is now optional
// Better handling of invisibility
// No more voting code
// Selective protection from humans or bots
// Fast loading (requires patch 4.30 or later)
// Dynamic Protection
 

//
//  Respawn protection vars
//
var config bool	    EnableRespawnProt;
var config int		ProtectTime;
var config float  	ScaleGlowBy;
var config bool 	ProtNoFrags;
var config bool		UseTranslucency;
var config bool		ProtFromCannon;
var config bool     ProtectHumans;
var config bool		ProtectBots;
var config bool     DynamicProt;
var config string	pmsg1;
var config string	pmsg2;	
var bool 			Initialized;
var int  			RespawnedIDs[32];
const          		ProtectorVersion 	= 2;
const          		ProtectorLevel   	= 30; 
const    			RefreshInterval 	= 1;
var bool			bTG;


//--------------------------------------------------------------------------
// Init some globals, send console msg, start timer 
//---------------------------------------------------------------------------
function PostBeginPlay()
{
	if (Initialized)
		return;
	Initialized = True;
	Level.Game.RegisterDamageMutator( Self );
	SetTimer(RefreshInterval,true);
	log( "***  Protector Mutator Initializing");
	log( "***  (c) 2000 Andrea Barlaam [Major Disaster]");
	log( "***  Version is " $ ProtectorVersion $ "." $ProtectorLevel);
	log( "***  Respawn Protection = " $ EnableRespawnProt);
	log( "***  Protector Mutator is now Operational");
	bTG =  Level.Game.bTeamGame;
}


//---------------------------------------------------------------
// Register a protected player
//---------------------------------------------------------------
function ModifyPlayer(Pawn Other)
{
	super.ModifyPlayer(Other);
	if (EnableRespawnProt == True)
		Protect(Other);
}


//---------------------------------------------------------------------------------
// Do not damage protected players and their victims
//---------------------------------------------------------------------------------
function MutatorTakeDamage( out int ActualDamage, Pawn Victim, Pawn InstigatedBy, out Vector HitLocation,
	out Vector Momentum, name DamageType)
{
	if (InstigatedBy.IsA('Bot') || 
		InstigatedBy.IsA('PlayerPawn') ||
		(InstigatedBy.IsA('TeamCannon') && ProtFromCannon == True ))
	{
		if (isProtected(Victim) || (isProtected(InstigatedBy) && ProtNoFrags == true))
			{
					ActualDamage = 0;
					momentum = vect(0,0,0);
			}
	}
	if ( NextDamageMutator != None )
		NextDamageMutator.MutatorTakeDamage( ActualDamage, Victim, InstigatedBy, HitLocation, Momentum, DamageType );
}



//---------------------------------------------------------------
//  Set properties of protected player
//---------------------------------------------------------------
function Protect(Pawn Other)
{
	local int k;
	k = Other.PlayerReplicationInfo.PlayerID;
	RespawnedIDs[k] = ProtectTime;
	if (UseTranslucency == True)
	{
		Other.Style = STY_Translucent;
		Other.ClientMessage(pmsg1);
		Other.ScaleGlow = ScaleGlowBy;
	}
}


//---------------------------------------------------------------
//  Protection has expired, reset player
//---------------------------------------------------------------
function StopGlow(pawn P)
{
	local Inventory Inv;
	Inv = P.FindInventoryType(class'UT_Invisibility');
	if ( Inv != None )
	{
		inv.Charge = 0;
		inv.GoToState('DeActivated');
	}
	if (UseTranslucency == True)
	{
		P.Style=P.default.Style;
		P.ScaleGlow = P.default.ScaleGlow;
		P.SetDefaultDisplayProperties(); // defensive
		P.Style = STY_Normal; // other defensive 
		P.ScaleGlow = 1.000000; // idem
	}
	P.clientmessage(pmsg2);
}

//--------------------------------------------------------------
// Expire protection in advance
//--------------------------------------------------------------
function bool HandlePickupQuery(Pawn Other, Inventory item, out byte bAllowPickup)
{
	local int k;
	if (IsProtected(Other) && DynamicProt == True)
	{	
		if (item.IsA('Weapon')          ||
		   	item.IsA('Udamage')         ||
		   	item.IsA('Healthpack')      ||
		   	item.IsA('Armor2')          || 
		   	item.IsA('ThightPad')       ||
		   	item.IsA('UT_Invisibility') ||
		   	item.IsA('UT_Shieldbelt')   )
		{
			k = Other.PlayerReplicationInfo.PlayerID;
			RespawnedIDs[k] = 1;
		}
	}
	if ( NextMutator != None )
		return NextMutator.HandlePickupQuery(Other, item, bAllowPickup);
	return false;
}

//---------------------------------------------------------------
//  Return protection status
//---------------------------------------------------------------
function bool isProtected(Pawn Other)
{
	local int k;
	if (Other.IsA('PlayerPawn') && !ProtectHumans)
		return False;
	if (Other.IsA('Bot') && !ProtectBots)
		return False;	
	k = Other.PlayerReplicationInfo.PlayerID;
	return (RespawnedIDs[k] > 0);
}


//---------------------------------------------------------------
//  Update protection status for all players
//---------------------------------------------------------------
function ProtStatusUpdate()
{ 
	local int k;
	local Pawn P;
	for( P=Level.PawnList; P!=None; P=P.nextPawn )     
	{
		k = P.PlayerReplicationInfo.playerID;
		if  (RespawnedIDs[k] > 0)
		{	
			RespawnedIDs[k] --;
			if (RespawnedIDs[k] < 1)
				StopGlow(P);
		}
	}
}


//---------------------------------------------------------------
//  Asynchronous task scheduler
//---------------------------------------------------------------
function Timer()
{
	if (EnableRespawnProt == True)
		ProtStatusUpdate();
}


