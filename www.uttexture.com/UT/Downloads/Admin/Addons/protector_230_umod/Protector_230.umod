function Created()
{
        bSizable=False;

        SetSize(320, 260);

        WinLeft=(Root.WinWidth-WinWidth)/2;
        WinTop=(Root.WinHeight-WinHeight)/2;

        Super.Created();
}

function ResolutionChanged(float W, float H)
{
        WinLeft=(Root.WinWidth-WinWidth)/2;
        WinTop=(Root.WinHeight-WinHeight)/2;

        Super.ResolutionChanged(W, H);
}

␀␀␀L 1␀␀␀␀␀␀␀␀␀␀a[class ProtectorClientWindow expands UMenuDialogClientWindow;


var UWindowSmallButton          OKButton;
var UWindowCheckbox             RespCheck;
var UWindowHSliderControl       vTimeSlider;
var UWindowHSliderControl       vGlowSlider;
var UWindowCheckbox             TransCheck;
var UWindowCheckbox             CannCheck;
var UWindowCheckbox             PBotCheck;
var UWindowCheckbox             PHumCheck;
var UWindowCheckbox             DynaCheck;

var int                         vTime;
var int							vGlow_n;
var float						vGlow;
var string                      OKHelp;
var string  					OKText;
var string                      vTimeHelp;
var string  					vTimeText;
var string                      vGlowHelp;
var string  					vGlowText;
var string                      vRespHelp;
var string  					vRespText;
var string                      vCannHelp;
var string  					vCannText;
var string                      vTransHelp;
var string  					vTransText;
var string                      vPBotHelp;
var string  					vPBotText;
var string                      vPHumHelp;
var string  					vPHumText;
var string                      vDynaHelp;
var string  					vDynaText;
var string  					PRDefaultHelp; 
var string 						PRDefaultText; 
 
function Created()
{
Super.Created();

	RespCheck = UWindowCheckbox(CreateControl(class'UWindowCheckbox', (WinWidth - (WinWidth/4)*3)/2, 10, ((WinWidth/4)*3)*0.45, 1));
	RespCheck.SetText(vRespText);
	RespCheck.SetHelpText(vRespHelp);
	RespCheck.bChecked=class'Protector230.Protector'.default.EnableRespawnProt;

	vTimeSlider=UWindowHSliderControl(CreateControl(class'UWindowHSliderControl', (WinWidth - (WinWidth/4)*3)/2, 35, (WinWidth/4)*3, 1));
	vTimeSlider.SetRange(1, 16, 1);
	vTime=class'Protector230.Protector'.default.ProtectTime;
	vTimeSlider.SetValue(vTime);
	vTimeSlider.SetText(vTimeText$"["$vTime$"]:");
	vTimeSlider.SetHelpText(vTimeHelp);
	
	vGlowSlider=UWindowHSliderControl(CreateControl(class'UWindowHSliderControl', (WinWidth - (WinWidth/4)*3)/2, 60, (WinWidth/4)*3, 1));
	vGlowSlider.SetRange(0, 40, 2);
	vGlow_n=class'Protector230.Protector'.default.ScaleGlowBy * 10;
	vGlowSlider.SetValue(vGlow_n);
	vGlowSlider.SetText(vGlowText$"["$vGlow_n$"]:");
	vGlowSlider.SetHelpText(vGlowHelp);
	
	TransCheck = UWindowCheckbox(CreateControl(class'UWindowCheckbox', (WinWidth - (WinWidth/4)*3)/2, 85, ((WinWidth/4)*3)*0.45, 1));
	TransCheck.SetText(vTransText);
	TransCheck.SetHelpText(vTransHelp);
	TransCheck.bChecked=class'Protector230.Protector'.default.UseTranslucency;
	
	CannCheck = UWindowCheckbox(CreateControl(class'UWindowCheckbox', (WinWidth - (WinWidth/4)*3)/2, 110, ((WinWidth/4)*3)*0.45, 1));
	CannCheck.SetText(vCannText);
	CannCheck.SetHelpText(vCannHelp);
	CannCheck.bChecked=class'Protector230.Protector'.default.ProtFromCannon;
		
	PHumCheck = UWindowCheckbox(CreateControl(class'UWindowCheckbox', (WinWidth - (WinWidth/4)*3)/2, 135, ((WinWidth/4)*3)*0.45, 1));
	PHumCheck.SetText(vPHumText);
	PHumCheck.SetHelpText(vPHumHelp);
	PHumCheck.bChecked=class'Protector230.Protector'.default.ProtectHumans;	
		
	PBotCheck = UWindowCheckbox(CreateControl(class'UWindowCheckbox', (WinWidth - (WinWidth/4)*3)/2, 160, ((WinWidth/4)*3)*0.45, 1));
	PBotCheck.SetText(vPBotText);
	PBotCheck.SetHelpText(vPBotHelp);
	PBotCheck.bChecked=class'Protector230.Protector'.default.ProtectBots;		
	
	DynaCheck = UWindowCheckbox(CreateControl(class'UWindowCheckbox', (WinWidth - (WinWidth/4)*3)/2, 185, ((WinWidth/4)*3)*0.45, 1));
	DynaCheck.SetText(vDynaText);
	DynaCheck.SetHelpText(vDynaHelp);
	DynaCheck.bChecked=class'Protector230.Protector'.default.DynamicProt;		
	
	OKButton = UWindowSmallButton(CreateControl(class'UWindowSmallButton', WinWidth-52, WinHeight-17, 48, 16));
	OKButton.SetText(OKText);
	OKButton.SetHelpText(OKHelp);
}

function Paint(Canvas C, float X, float Y)
{
        local Texture T;
        Super.Paint(C, X, Y);
        T = GetLookAndFeelTexture();
        DrawUpBevel( C, 0, WinHeight-20, WinWidth, 20, T);
}

function Notify(UWindowDialogControl C, byte E)
{
        Super.Notify(C, E);
        switch(E)
        {
        case DE_Click:
                switch (C)
                {
                case OKButton:
                        Close();
                        break;
                }
        case DE_Change:
                switch(C)
                {
                case vTimeSlider:
                        vTimeChanged();
                        break; 
                case vGlowSlider:
                        vGlowChanged();
                        break;         
                }
        }
}

function vTimeChanged()
{
        vTime=vTimeSlider.GetValue();
        vTimeSlider.SetText(vTimeText@"["$vTime@"]:");
}

function vGlowChanged()
{
        vGlow_n = vGlowSlider.GetValue();
        vGlowSlider.SetText(vGlowText@"["$vGlow_n@"]:");
        vGlow = vGlow_n / 10;
}


function Close(optional bool bByParent)
{
        vGlow = vGlow_n / 10;
        class'Protector230.Protector'.default.ProtectTime=vTime;
        class'Protector230.Protector'.default.ScaleGlowBy=vGlow;
        class'Protector230.Protector'.default.UseTranslucency=TransCheck.bChecked;
        class'Protector230.Protector'.default.EnableRespawnProt=RespCheck.bChecked;
        class'Protector230.Protector'.default.ProtFromCannon=CannCheck.bChecked;
		class'Protector230.Protector'.default.ProtectHumans=PHumCheck.bChecked;
		class'Protector230.Protector'.default.ProtectBots=PBotCheck.bChecked;
		class'Protector230.Protector'.default.DynamicProt=DynaCheck.bChecked;
        class'Protector230.Protector'.SaveConfig();
        class'Protector230.Protector'.static.StaticSaveConfig();
		
        Super.Close(bByParent);
}

